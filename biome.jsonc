{
  "$schema": "https://biomejs.dev/schemas/2.1.2/schema.json",
  "extends": [
    "ultracite"
  ],
  "linter": {
    "rules": {
      "nursery": {
        "noUnknownAtRule": "info"
      },
      "suspicious": {
        "noEmptyInterface": "off",
        "noArrayIndexKey": "off"
      },
      "performance": {
        "noNamespaceImport": "off"
      }
    }
  }
}